      <scriptlet id="0e2be093-3262-4af0-a71f-dfe49915a9a8">
        <annotation></annotation>
        <name>Step Remote Command in Flow Largest File (308cffc5-3c7a-42c9-9d7f-ff1cceaf20c7)</name>
        <script>protocol = scriptletContext.get(&quot;protocol&quot;);
out = scriptletRawResult[&apos;stdOut&apos;];
error = scriptletRawResult[&apos;exception&apos;];
result = scriptletRawResult[&apos;Result&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];
err = scriptletRawResult[&apos;stdErr&apos;];

if (returnCode == -1)
{
    if (error == null)
    {
        if (result == &quot;&quot;)
            error = &quot;Could not find largest file. Probable Cause: Invalid directory name&quot;
        else
            error = &quot;Could not find largest file. &quot; + err;
     }
    scriptletContext.put(&quot;Error String&quot;, error);
}
//Since ssh does not return the returnCode for failures for this command, 
//only stdErr is set and hence the below if condition
else if ((err != null) &amp;&amp; (err != &quot;&quot;))
{
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, err);
}

else if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; (error == null))
{

    if ((result == null) || (result == &quot;&quot;))
    {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, &quot;Could not find largest file. Probable Cause: Invalid directory name&quot;);
     }
    if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
    {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, out);
     }
     if (result.toLowerCase().indexOf(&quot;no such file or directory&quot;) != -1)
     {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, out);     
     }
     if (result.toLowerCase().indexOf(&quot;cannot execute&quot;) != -1)
     {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, out);     
     }
     if (result.toLowerCase().indexOf(&quot;illegal option&quot;) != -1)
     {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, out);     
     }
     if (result.toLowerCase().indexOf(&quot;permission denied&quot;) != -1)
     {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, out);     
     }             
     
}</script>
        <type>RHINO</type>
      </scriptlet>
