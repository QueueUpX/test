      <scriptlet id="6b2ec948-431e-4c43-b851-62add0e9ebb3">
        <annotation></annotation>
        <name>Step Remote Command in Flow Run Service (a72a58af-bf7b-4e6d-b014-89ec6be448e9)</name>
        <script>protocol = scriptletContext.get(&quot;protocol&quot;);
error = scriptletRawResult[&apos;exception&apos;];
result = scriptletRawResult[&apos;Result&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];
stdErr = scriptletRawResult[&apos;stdErr&apos;];
if (returnCode == -1)
{
    if ((error == null) || (error == &quot;&quot;))
    {
        if ((stdErr == &quot;&quot; ) || (stdErr == null))
            error = &quot;Could not run service&quot;;
        else  
           error = stdErr;
     }
     scriptletContext.put(&quot;Error String&quot;, error);
}
else if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; (error == null))
{
    if (result.toLowerCase().indexOf(&quot;no such file or directory&quot;) != -1)
    {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, result);
     }
      if (result.toLowerCase().indexOf(&quot;cannot execute&quot;) != -1)
      {
          scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;Error String&quot;, result);   
      }
      if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
      {
          scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;Error String&quot;, result);         
      }
      if (result.toLowerCase().indexOf(&quot;permission denied&quot;) != -1)
      {
          scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;Error String&quot;, result);         
      }
      
}</script>
        <type>RHINO</type>
      </scriptlet>
