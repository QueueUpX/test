        <script>var ipVersion = scriptletContext.get(&quot;ipVersion&quot;);
var pingCommand = &quot;ping&quot;;

if(!((ipVersion == &quot;&quot;) || (typeof(ipVersion) == &apos;undefined&apos;))){
	if (ipVersion.trim().equals(&quot;6&quot;)){
		pingCommand = &quot;ping6&quot;;
	}
}

