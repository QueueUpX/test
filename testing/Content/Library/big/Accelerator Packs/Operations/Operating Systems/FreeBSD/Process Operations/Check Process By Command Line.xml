  <scriptlet id="d68851d4-491d-4b38-8b3f-cd70ce6fb0cf">
    <annotation></annotation>
    <name>Operation Check Process By Command Line (f117aa36-6816-484d-8ea9-7d571c2744ae)</name>
    <script>protocol = scriptletContext.get(&quot;protocol&quot;);
result = new java.lang.String(scriptletRawResult[&apos;stdOut&apos;]);
if (result == null || result.trim() == &quot;&quot;)
{
    scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
    scriptletResponse = &quot;failure&quot;;
    scriptletResult = service+&quot; is not a valid service name or is not running&quot;;
}
else if (result.indexOf(&quot;annot read termcap database&quot;) &gt; -1)
{
    scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
    scriptletResponse = &quot;failure&quot;;
    scriptletResult = scriptletRawResult[&apos;stdOut&apos;];
}</script>
    <type>RHINO</type>
  </scriptlet>
