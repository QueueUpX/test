      <scriptlet id="34420c1c-3d17-4ba2-b87f-36be2f896d24">
        <annotation></annotation>
        <name>Step Remote Command in Flow Delete File (df0e8725-1c73-4c80-a643-11ba4ad11f16)</name>
        <script>protocol = scriptletContext.get(&quot;protocol&quot;);
error = scriptletRawResult[&apos;exception&apos;];
result = scriptletRawResult[&apos;stdOut&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];
stdErr = scriptletRawResult[&apos;stdErr&apos;];

if (returnCode == -1)
{
    if ((error == null) || (error == &quot;&quot;))
    {
        if ((stdErr == &quot;&quot;) || (stdErr == null))
            error =  &quot;Could not delete the file&quot;;
        else
            error = stdErr;
     }
     scriptletContext.put(&quot;stdErr&quot;, error);
}
else if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; (error == null))
{
    //NOn-existent file&apos;s name is given
    // Also if the &quot;rm&quot; command is not in the PATH
    if (result.toLowerCase().indexOf(&quot;file or directory in the path name does not exist&quot;) != -1)
    {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, result);
     }
     //If a file created by root is being attempted to be removed by a non-root user.
     if (result.toLowerCase().indexOf(&quot;file access permissions do not allow the specified action&quot;) != -1)
     {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, result);      
      }
      if (result.toLowerCase().indexOf(&quot;cannot remove&quot;) != -1)
      {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, result);      
      }
      //When the command executable access permission is not present.
      if (result.toLowerCase().indexOf(&quot;permission denied&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
      //If the filename entered starts with a hyphen
      if (result.toLowerCase().indexOf(&quot;not a recognized flag&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);         
      }
      //If the filename is given as one of the option that the command supports,eg.,-i
      if (result.toLowerCase().indexOf(&quot;usage&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
      //If the command is not found in PATH and is tried to be executed in KSH
      if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
      //If &quot;rm: Remove &lt;filename&gt;? prompt appears
      if (result.toLowerCase().indexOf(&quot;remove&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, &quot;Could not remove the file. Probable Cause: Permission issue.&quot;);   
      }
}</script>
        <type>RHINO</type>
      </scriptlet>
