      <scriptlet id="b8c85dbd-86d8-42d4-9394-da5717164ba3">
        <annotation></annotation>
        <name>Step Remote Command in Flow Kill-9  Process (6c4aa4de-0a42-41ca-b3d7-738e653a3c21)</name>
        <script>result = scriptletRawResult[&apos;returnResult&apos;];
PID = scriptletContext.get(&quot;PID&quot;);
protocol = scriptletContext.get(&quot;protocol&quot;);
error = scriptletRawResult[&apos;exception&apos;];
stdErr = scriptletRawResult[&apos;stdErr&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];
if (returnCode == -1)
{
    if ((error == null) || (error == &quot;&quot;))
    {
        if ((stdErr == &quot;&quot; ) || (stdErr == null))
            error = &quot;Could not kill process&quot;;
        else  
           error = stdErr;
     }
     scriptletContext.put(&quot;stdErr&quot;, error);
}
if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; ( error == null))
{
    if ((result.indexOf(&quot;process does not exist&quot;) != -1) || (result.indexOf(&quot;No such process&quot;) != -1) || (result.indexOf(&quot;job does not exist&quot;) != -1) || (result.indexOf(&quot;Specify a process identifier&quot;) != -1) || (result.indexOf(&quot;Arguments must be&quot;) != -1))
    {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, result);
    }
    if (result.toLowerCase().indexOf(&quot;file or directory in the path name does not exist&quot;) != -1)
   {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, result);
   }
   if (result.toLowerCase().indexOf(&quot;file access permissions do not allow the specified action&quot;) != -1)
   {
        scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
        scriptletContext.put(&quot;stdErr&quot;, result);      
   }
   if (result.toLowerCase().indexOf(&quot;permission denied&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
      if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
      if (result.toLowerCase().indexOf(&quot;usage&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
      }
   if (result.toLowerCase().indexOf(&quot;invalid signal specification&quot;) != -1)
   {
        scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
        scriptletContext.put(&quot;stdErr&quot;, result);      
   }
   if (result.toLowerCase().indexOf(&quot;flag is not valid for this command&quot;) != -1)
   {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, result);   
   }
   if (result.toLowerCase().indexOf(&quot;arguments must be process or job ids&quot;) != -1)
   {
        scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
        scriptletContext.put(&quot;stdErr&quot;, result);      
   }
    if (result.toLowerCase().indexOf(&quot;specify a process identifier&quot;) != -1)
    {
	    scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
	    scriptletContext.put(&quot;stdErr&quot;, result);   
    }
}
</script>
