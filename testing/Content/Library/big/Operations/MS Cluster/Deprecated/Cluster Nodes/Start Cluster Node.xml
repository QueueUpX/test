      <scriptlet id="cb2c1fd9-836d-47dc-a088-c90265da428d">
        <annotation></annotation>
        <name>Step Validate Fix Quorum in Flow Start Cluster Node (905ed373-0930-4266-a4da-b79e7169b1d5)</name>
        <script>var fixQuorum = scriptletContext.get(&quot;fixQuorum&quot;).toLowerCase();

if ( (typeof(fixQuorum) == &apos;undefined&apos;) || (fixQuorum == &quot;&quot;) )
{
    fixQuorum = &quot;false&quot;;
    scriptletContext.put(&quot;fixQuorum&quot;,&quot;False&quot;);
}

if ( (fixQuorum != &quot;true&quot;) &amp;&amp; (fixQuorum != &quot;false&quot;) )
    scriptletResult = &quot;Invalid value entered for the fixQuorum input.&quot;;</script>
