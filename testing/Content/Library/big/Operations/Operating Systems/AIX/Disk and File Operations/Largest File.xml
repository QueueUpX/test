      <scriptlet id="e0e7fc40-2593-46a1-b7fa-ed1b07ad8b19">
        <annotation></annotation>
        <name>Step Remote Command in Flow Largest File (d6605d18-d819-4bd2-8a79-172823fce0c6)</name>
        <script>protocol = scriptletContext.get(&quot;protocol&quot;);
out = scriptletRawResult[&apos;stdOut&apos;];
error = scriptletRawResult[&apos;exception&apos;];
result = scriptletRawResult[&apos;Result&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];
stdErr = scriptletRawResult[&apos;stdErr&apos;];

if (returnCode == -1)
{
    if ((error == null) || (error == &quot;&quot;))
    {
        if ((out == &quot;&quot;) || (out == null))
            error = &quot;Could not find largest file. Probable Cause: Invalid directory name&quot;
        else
            error = &quot;Could not find largest file.&quot; + stdErr;
     }
            
    scriptletContext.put(&quot;stdErr&quot;, error);
}
//Since ssh does not return the returnCode for failures for this command, 
//only stdErr is set 
else if ((stdErr != null) &amp;&amp; (stdErr != &quot;&quot;))
{
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, stdErr);
}
else if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; (error == null))
{
    if ((result == null) || (result == &quot;&quot;))
    {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, &quot;Could not find largest file. Probable Cause: Invalid directory name&quot;);
    }
    if (result.toLowerCase().indexOf(&quot;does not exist&quot;) != -1)
    {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, out);
     }
     if (result.toLowerCase().indexOf(&quot;file or directory in the path name does not exist&quot;) != -1)
     {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, out);
     }
      if (result.toLowerCase().indexOf(&quot;file access permissions do not allow the specified action&quot;) != -1)
     {
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;stdErr&quot;, out);      
      }
      if (result.toLowerCase().indexOf(&quot;permission denied&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, out);   
      }
      if (result.toLowerCase().indexOf(&quot;usage&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, out);   
      }
      if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
      {
          scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
          scriptletContext.put(&quot;stdErr&quot;, out);   
      }
} </script>
        <type>RHINO</type>
      </scriptlet>
