      <scriptlet id="373fe0d3-192d-4b24-91f3-365b171e3e35">
        <annotation></annotation>
        <name>Step Remote Command in Flow Check Process by PID (b403e717-6a2e-4c8f-ac4d-ff21ea0c3747)</name>
        <script>output = scriptletRawResult[&apos;returnResult&apos;];
PID = scriptletContext.get(&quot;PID&quot;);
protocol = scriptletContext.get(&quot;protocol&quot;);
error = scriptletRawResult[&apos;exception&apos;];
if ((protocol.indexOf(&quot;Telnet&quot;) != -1) &amp;&amp; (error == null))
{
    if (output.indexOf(&quot;Error:&quot;) != -1)
    {
        scriptletResponse = &quot;failure&quot;;
        scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
        scriptletResult = output;        
     }
     else if (output.indexOf(PID) == -1) 
     {
         scriptletResponse = &quot;failure&quot;;
         scriptletContext.put(&quot;returnCode&quot;, &quot;-1&quot;);
         scriptletResult = PID + &quot; - No such process&quot;;
      }
}
</script>
        <type>RHINO</type>
      </scriptlet>
