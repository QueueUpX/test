    <name>Operation Reboot server (e3a375ef-39a6-4c25-a5d2-616ff7bedcc4)</name>
    <script>// set up defaults
scriptletResult = &quot;-1&quot;;
scriptletResponse = &quot;failure&quot;;
code = parseInt(scriptletRawResult[&apos;Code&apos;]);
if (code == 0 || ((scriptletRawResult[&apos;Error String&apos;] == null || scriptletRawResult[&apos;Error String&apos;] == &quot;&quot;)&amp;&amp; (scriptletRawResult[&apos;FailureMessage&apos;] == null || scriptletRawResult[&apos;FailureMessage&apos;] == &quot;&quot;))) {
scriptletResponse = &quot;success&quot;;
}
else
scriptletResponse = &quot;failure&quot;;
</script>
