      <scriptlet id="4ab5ef4e-aa19-479d-83fc-467c45c6164d">
        <annotation></annotation>
        <name>Step Remote Command in Flow Check Network Interface for Errors (40ed4791-77df-46d9-acae-998a4835e7a7)</name>
        <script>protocol = scriptletContext.get(&quot;protocol&quot;);
error = scriptletRawResult[&apos;exception&apos;];
result = scriptletRawResult[&apos;Result&apos;];
returnCode = scriptletRawResult[&apos;returnCode&apos;];

if (returnCode == -1)
{
    scriptletContext.put(&quot;Error String&quot;, error);
}
else if ((protocol.indexOf(&quot;Telnet&quot;) != -1) || (error != null))
{
    if (result.toLowerCase().indexOf(&quot;not found&quot;) != -1)
    {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, result);
     }
     if (result.toLowerCase().indexOf(&quot;cannot execute&quot;) != -1)
      {
         scriptletContext.put(&quot;Code&quot;, &quot;-1&quot;);
         scriptletContext.put(&quot;Error String&quot;, result);      
      }
}</script>
        <type>RHINO</type>
      </scriptlet>
