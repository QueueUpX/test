  <scriptlet id="82f9372d-e589-4bd2-846a-c1ac8bcfdf47">
    <annotation></annotation>
    <name>Operation Uptime (1ec201bc-4d75-4d4a-8951-550792f2acad)</name>
    <script>// this will parse the uptime output into a consistent format and return the number
// of minutes the system has been up
// the supported output formats are:
// 3 min(s)
// 2:34
// 12 day(s) 18:32 
// 12 day(s) 10 min(s)

// set up a default
minutes = -1;

// make sure it worked
code = scriptletRawResult[&apos;Code&apos;];
if (parseInt(code) == 0) {
   // get the output string
   output = scriptletRawResult[&apos;Output String&apos;];

   // first check to see if the output string has &quot;min(s)&quot; in it
   parseline = output.match(/(\d*)(\s*)(min\(s\))/);
   if (parseline != null) {
      minutes = parseInt(parseline[1],10);
   }
   else {
      // get the hours and minutes
      parseline = output.match(/(\d*)(:)(\d*)(,.*)/);
      minutes = (parseInt(parseline[1],10) * 60) + parseInt(parseline[3],10);
   }

   // see if there are any days
   days = output.match(/(\d*)(\s*)(day)/);
   if (days != null) minutes += parseInt(days[1],10) * 60 * 24;
}
scriptletResult = minutes.toString();
</script>
    <type>RHINO</type>
  </scriptlet>
